--TEST--
"countBy" filter
--TEMPLATE--
{% for key, value in data|countBy(=> _.foo ) %}
{{ key }}: {{ value }}
{% endfor %}
--DATA--
return [ 'data' => [
    [ 'foo' => 'foo' ],
    [ 'foo' => 'foo' ],
    [ 'foo' => 'foobar' ],
    [ 'foo' => 'bar' ],
    [ 'foo' => 'bar' ],
    [ 'foo' => 'foo' ],
] ];
--EXPECT--
foo: 3
foobar: 1
bar: 2
--DATA--
return [ 'data' => [
    [ 'foo' => null, 'bar' => 1 ],
    [ 'foo' => null, 'bar' => 2 ],
    [ 'foo' => true ],
    [ 'foo' => false ],
    [ 'foo' => false ],
    [ 'foo' => null, 'bar' => [] ],
] ];
--EXPECT--
null: 3
true: 1
false: 2
--DATA--
return [ 'data' => [
    [ 'foo' => 1 ],
    [ 'foo' => 1 ],
    [ 'foo' => -1 ],
    [ 'foo' => 1.2 ],
    [ 'foo' => -1.3 ],
    [ 'foo' => 1.9 ],
] ];
--EXPECT--
1: 4
-1: 2