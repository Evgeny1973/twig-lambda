--TEST--
"unique" filter
--TEMPLATE--
{% for i in data|unique %}
 * {{ i }}
{% endfor %}
--DATA--
return [ 'data' => [1, 2, 3, 4, 5] ];
--EXPECT--
 * 1
 * 2
 * 3
 * 4
 * 5
--DATA--
return [ 'data' => [1, 2, 1, 3] ];
--EXPECT--
 * 1
 * 2
 * 3
--DATA--
return [ 'data' => new ArrayIterator([1, 3, 1, 2, 3]) ];
--EXPECT--
 * 1
 * 3
 * 2
--DATA--
return [ 'data' => ['foo', 'bar', 'foo'] ]
--EXPECT--
 * foo
 * bar