{
    "name": "leonaero/twig-lambda",
    "type": "library",
    "description": "Lambda expressions for Twig and filters that make use of them",
    "keywords": [
        "function",
        "unique",
        "functional",
        "group",
        "extensions"
    ],
    "prefer-stable": true,
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "LeonAero\\TwigLambda\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
